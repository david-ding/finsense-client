{
  "name": "finsense-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "docker:build": "docker buildx build --platform linux/arm64 -t davidding/finsense-client .",
    "docker:publish": "docker push davidding/finsense-client",
    "docker:bp": "docker buildx build --platform linux/arm64 --push -t davidding/finsense-client ."
  },
  "devDependencies": {
    "@imask/svelte": "7.6.1",
    "@playwright/test": "1.47.2",
    "@reduxjs/toolkit": "2.2.7",
    "@sveltejs/adapter-node": "5.2.5",
    "@sveltejs/kit": "2.6.1",
    "@sveltejs/vite-plugin-svelte": "3.1.2",
    "@tailwindcss/forms": "0.5.9",
    "@types/cookie": "0.6.0",
    "@types/lodash-es": "4.17.12",
    "@typescript-eslint/eslint-plugin": "8.7.0",
    "@typescript-eslint/parser": "8.7.0",
    "autoprefixer": "10.4.20",
    "cookie": "0.6.0",
    "cssnano": "7.0.6",
    "dayjs": "1.11.13",
    "dotenv": "16.4.5",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.44.1",
    "lodash-es": "4.17.21",
    "postcss": "8.4.47",
    "postcss-load-config": "6.0.1",
    "prettier": "3.3.3",
    "prettier-plugin-svelte": "3.2.7",
    "redux": "5.0.1",
    "rxjs": "7.8.1",
    "svelte": "4.2.19",
    "svelte-check": "4.0.4",
    "svelte-preprocess": "6.0.3",
    "svelte-selectbox": "0.2.1",
    "tailwindcss": "3.4.13",
    "tslib": "2.7.0",
    "typescript": "5.6.2",
    "vite": "5.4.8"
  },
  "type": "module"
}
